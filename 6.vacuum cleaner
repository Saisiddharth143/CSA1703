class VacuumCleaner:
    def __init__(self):
        self.location = 0  
        self.status = ["Dirty", "Dirty"]  
        self.actions = []
    def clean(self):
        if self.status[self.location] == "Dirty":
            self.status[self.location] = "Clean"
            self.actions.append(f"Cleaned Room {chr(ord('A') + self.location)}")
        else:
            self.actions.append(f"Room {chr(ord('A') + self.location)} is already clean")
    def move(self):
        if self.location == 0:
            self.location = 1
            self.actions.append("Moved to Room B")
        else:
            self.location = 0
            self.actions.append("Moved to Room A")
    def check_status(self):
        return self.status
    def perform_actions(self):
        while "Dirty" in self.status:
            self.clean()
            self.move()
        return self.actions
if __name__ == "__main__":
    cleaner = VacuumCleaner()
    actions = cleaner.perform_actions()
    print("\n".join(actions))
